{
  "uid": "rpc-aggregator-main",
  "id": null,
  "title": "RPC Aggregator Dashboard",
  "tags": ["rpc", "aggregator"],
  "style": "dark",
  "timezone": "browser",
  "schemaVersion": 39,
  "panels": [
    {
      "id": 4,
      "title": "Cache Hit Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(rpc_cache_hits_total[5m]) / (rate(rpc_cache_hits_total[5m]) + rate(rpc_cache_misses_total[5m])) * 100",
          "legendFormat": "Cache Hit Rate",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "green", "value": 90 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "title": "Upstream Errors",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(rpc_upstream_errors_total[5m])",
          "legendFormat": "{{upstream}} - {{error_type}}",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "yAxes": [
        {
          "label": "Errors/sec",
          "min": 0
        }
      ]
    },
    {
      "id": 6,
      "title": "Upstream Health",
      "type": "table",
      "targets": [
        {
          "expr": "rpc_upstream_health",
          "legendFormat": "{{upstream}}",
          "datasource": { "type": "prometheus", "uid": "prometheus" }
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "job": true,
              "instance": true
            },
            "renameByName": {
              "upstream": "Upstream",
              "Value": "Health Status"
            }
          }
        }
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s",
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "editable": true,
  "graphTooltip": 0,
  "links": []
}
